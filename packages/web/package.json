{
  "name": "@codeponder/web",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000/graphql",
    "codegen:generate": "apollo codegen:generate --queries=./graphql/**/*.ts --schema=./schema.json --outputFlat --target=typescript ./lib/schema-types.ts",
    "generate": "npm run schema:download && npm run codegen:generate"
  },
  "dependencies": {
    "@codeponder/common": "1.0.0",
    "@codeponder/ui": "1.0.0",
    "apollo-boost": "^0.1.21",
    "formik": "^1.3.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "next-routes": "^1.4.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.1",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@types/next": "^7.0.1",
    "@types/react": "^16.4.16",
    "@types/react-dom": "16.0.9",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "file-loader": "^2.0.0",
    "graphql-code-generator": "^0.17.0",
    "graphql-codegen-typescript-client": "^0.14.1",
    "graphql-codegen-typescript-common": "^0.14.1",
    "graphql-codegen-typescript-react-apollo": "^0.14.1",
    "lerna": "^3.4.3",
    "nodemon": "^1.18.9",
    "ts-node": "^7.0.1",
    "typescript": "3.1.6",
    "url-loader": "^1.1.2"
  },
  "license": "ISC"
}
